{
  "manifest_version": 2,
  "name": "Container proxy",
  "version": "0.1.16",
  "description": "__MSG_General_extensionDescription__",
  "default_locale": "en",
  "applications": {
    "gecko": {
      "id": "contaner-proxy@bekh-ivanov.me",
      "strict_min_version": "77.0"
    }
  },
  "browser_action": {
  },
  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true,
    "browser_style": false
  },

  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "contextualIdentities",
    "cookies",
    "proxy"
  ],

  "background": {
    "scripts": [
      "background/index.js"
    ]
  }

}
